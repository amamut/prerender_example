        "redis": "^3.1.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.2",
        "@types/node": "^13.1.8",
        "@types/redis": "^2.8.14",
        "concurrently": "^5.0.2",
        "tslint": "^5.20.1",
        "typescript": "^3.7.5"
    }
}
